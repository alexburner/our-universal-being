  },
  "devDependencies": {
    "@types/d3-force": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.4.1",
    "typescript": "4.4.4",
    "vite": "2.6.14",
    "vite-plugin-checker": "0.3.4"
  },
  "dependencies": {
    "d3-force": "3.0.0"
  }
}
